m = 20; n = 10; p = 4;
A = randn(m,n); b = randn(m,1);
C = randn(p,n); d = randn(p,1); e = rand;
cvx_begin
    variable x(n)
    minimize( norm( A * x - b, 2 ) )
    subject to
        C * x == d
        norm( x, Inf ) <= e
cvx_end


sigma = [1 0.0015 -0.05;
         0.0015 1 -0.1;
         -0.02 -0.1 1];
b = [ 0.1594, 0.0126, 0.8282]';
n = 3;

cvx_begin
    variable x(n)
    minimize(0.5 * x' * sigma * x - b'*log(x))